{
  "html.filetypes": ["html","handlebars","htmldjango","blade","erb","eruby","eelixir"],
  "html.format.templating": true,
  "solargraph.diagnostics": true,
  "solargraph.formatting": true,
  "elixir.pathToElixirLS": "~/projects/elixir-ls/release/language_server.sh",
  "snippets.ultisnips.directories": [
    "UltiSnips"
  ],
  "languageserver": {
    "clojure-lsp": {
      "command": "bash",
      "args": ["-c", "clojure-lsp"],
      "filetypes": ["clojure"],
      "rootPatterns": ["project.clj"],
      "additionalSchemes": ["jar", "zipfile"],
      "trace.server": "verbose",
      "initializationOptions": {
      }
    }
  }
}
